.config/secrets/**
~/.ssh/
.ssh/
.ssh/config
.ssh/config.d/
.ssh/known_hosts
.ssh/authorized_keys
.ssh/id_*
.ssh/identity*
.gitconfig
.gitconfig.local
*.env
*.env.*
*.env.local
*.env.*.local
.env.*.php
.env.production.local
.env.*.local
.env.local.php
credentials.*
credentials/**
.credentials
.credentials/**

